#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int x, y, z;
        scanf("%d %d %d", &x, &y, &z);
        // Your code goes here
         if((x*5+y*10)>z)
            printf("%d\n",(x*5+y*10)/z);
        else if((x*5+y*10)==z)
            printf("1\n");
        else
            printf("0\n");
    }

}
